{
    "extends": "./tsconfig.json",
    "exclude": ["./src/utils/trpc.*"],
    "compilerOptions": {
        // Disable TRPC errors
        "noImplicitAny": false,
        "paths": {
            "@/utils/trpc": ["./src/utils/trpcMock"],
            "@/*": ["./src/*"],
            "~shared/*": ["./../shared/*"]
        }
    }
}